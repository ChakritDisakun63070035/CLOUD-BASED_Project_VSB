        <script>
        var app = new Vue({
            el: "#app",
            data: {
                courses: JSON.parse(`<%- courses %>`),
                users: JSON.parse(`<%- users %>`),
                admins: JSON.parse(`<%- admin %>`),
                sorted: "",
                idCounter: 16,
            },
            // created() {
            //     this.idCounter = this.courses.length
            //     console.log(this.courses.length)
            // this.filteredTasks = this.tasks
            // this.courses = this.courses
            // `this` points to the vm instance
            // console.log('a is: ' + this.a)
            //   },
            computed: {
                computedCourses() {
                    let results = this.courses
                    return results.map((b) => {
                        if (b.course_info.length > 90) {
                            b.course_info = b.course_info.substring(0, 90) + "..."
                        }
                        return b
                    })
                },
                filteredTasks() {
                    // get: function compareAlpha(a, b) {
                    //     return this.$store.state.sorted.filteredTasks
                    //     }
                    function compareAlpha(a, b) {
                        if (a.course_name < b.course_name) {
                            return -1
                        } else if (a.course_name > b.course_name) {
                            return 1
                        } else {
                            return 0
                        }
                    }
                    function compareID(a, b) {
                        if (a.course_id < b.course_id) {
                            return -1
                        } else if (a.course_id > b.course_id) {
                            return 1
                        } else {
                            return 0
                        }
                    }
                    function comparePrice(a, b) {
                        if (a.course_price < b.course_price) {
                            return -1
                        } else if (a.course_price > b.course_price) {
                            return 1
                        } else {
                            return 0
                        }
                    }
                    if (this.sorted == "alpha") {
                        return this.courses.sort(compareAlpha)
                    } else if (this.sorted == "sortID") {
                        return this.courses.sort(compareID)
                    } else if (this.sorted == "price") {
                        return this.courses.sort(comparePrice)
                    }
                },
            },
        })
    </script>
